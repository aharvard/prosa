{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "prettier",
        "react"
    ],
    "rules": {
        "import/prefer-default-export": 0,
        "react/jsx-props-no-spreading": 0,
        "react/forbid-prop-types": 0,
        "no-nested-ternary": 0,
        "no-use-before-define": 0,
        "no-param-reassign": 0,
        "no-shadow": 0
    }
}
